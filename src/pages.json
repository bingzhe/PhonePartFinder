{
  "globalStyle": {
    "navigationStyle": "default",
    "navigationBarTitleText": "MOSTON",
    "navigationBarBackgroundColor": "#ffffff",
    "navigationBarTextStyle": "black",
    "backgroundColor": "#FFFFFF"
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
    }
  },
  "tabBar": {
    "color": "#6e6d6b",
    "selectedColor": "#e64340",
    "backgroundColor": "#ffffff",
    "borderStyle": "white",
    "height": "50px",
    "fontSize": "10px",
    "iconWidth": "24px",
    "spacing": "3px",
    "list": [
      {
        "iconPath": "static/tabbar/home-off.png",
        "selectedIconPath": "static/tabbar/home-on.png",
        "pagePath": "pages/index/index",
        "text": "首页"
      },
      {
        "iconPath": "static/tabbar/video-off.png",
        "selectedIconPath": "static/tabbar/video-on.png",
        "pagePath": "pages/video-course/video-course",
        "text": "教程"
      },
      {
        "iconPath": "static/tabbar/feedback-off.png",
        "selectedIconPath": "static/tabbar/feedback-on.png",
        "pagePath": "pages/feedback/feedback",
        "text": "反馈"
      }
    ]
  },
  "pages": [
    {
      "path": "pages/index/index",
      "type": "home",
      "style": {
        "navigationBarTitleText": "鑫嵩贸易商行"
      }
    },
    {
      "path": "pages/about/about",
      "type": "page",
      "style": {
        "navigationBarTitleText": "关于"
      }
    },
    {
      "path": "pages/feedback/feedback",
      "type": "page",
      "style": {
        "navigationBarTitleText": "反馈",
        "backgroundColor": "#f9f9f9"
      }
    },
    {
      "path": "pages/video-course/video-course",
      "type": "page",
      "style": {
        "navigationBarTitleText": "教程"
      }
    },
    {
      "path": "pages/yhxy-page/yhxy-page",
      "type": "page",
      "style": {
        "navigationBarTitleText": "用户协议"
      }
    },
    {
      "path": "pages/ysxy-page/ysxy-page",
      "type": "page",
      "style": {
        "navigationBarTitleText": "隐私协议"
      }
    }
  ],
  "subPackages": []
}